{
  "expo": {
    "name": "Yoman",
    "orientation": "portrait",
    "newArchEnabled": true,
    "userInterfaceStyle": "dark",
    "slug": "yoman-e-commerce",
    "scheme": "reactnativeboilerplate",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "icon": "./src/assets/splash-icon.png",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.yoman",
      "edgeToEdgeEnabled": true,
      "versionCode": 2
    },
    "ios": {
      "bundleIdentifier": "com.reactnativeboilerplate"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./src/assets/splash-icon.png",
          "dark": {
            "image": "./src/assets/splash-icon.png"
          },
          "imageWidth": 200
        }
      ]

    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27b6b398-1c4b-4ec6-a41a-af95b97ebe9e"
      }
    },
    "owner": "aahilalwani"
  }
}
